# class definition test
class MyString:

    def __init__(self):
        pass


print("===== MyString Class")
s = MyString()
print(type(s))

print("s의 클래스 확인:", s.__class__)
print("MyString의 부모 확인:", MyString.__bases__)  # 여러 부모로부터 상속을 받을 수 있다.

print("s의 구성 요소:", dir(s))
print("object의 구서오 요소:", dir(object))
